{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#d8dee9",
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }}",
          "type": "time"
        },
        {
          "foreground": "#56B6C2",
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path",
          "properties": {
            "style": "full"
          }
        },
        {
          "foreground": "#D0666F",
          "properties": {
            "branch_icon": "",
            "fetch_status": false
          },
          "style": "plain",
          "template": "<#5FAAE8>git:(</>{{ .HEAD }}<#5FAAE8>) </>",
          "type": "git"
        },
        {
          "foreground": "#DCB977",
          "style": "plain",
          "template": " \uf119",
          "type": "exit"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "<#88c0d0>\u276f</><#88c0d0>\u276f</> ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
